<script>

    let tab = document.getElementsByClassName('tab-links');
    let tabcon = document.getElementsByClassName('tab-contents');
    
    
    function opentab(tabname){
        for(let tab of tab){
            tab.classList.removeClass("active");
    
        }
        for(let tabcon of tabcon){
            tabcon.classList.removeClass("active-tab");
    
        }
        event.currentTarget.classList.addClass("active")
        document.getElementById(tabname).classList.addClass("active-tab")
    
    }
    
    </script>
